{
  "name": "create-tauri-app",
  "version": "2.7.3",
  "description": "Rapidly scaffold out a new tauri app project.",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/tauri"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tauri-apps/create-tauri-app.git"
  },
  "contributors": [
    "Tauri Team <team@tauri-apps.org> (https://tauri.app)"
  ],
  "bugs": {
    "url": "https://github.com/tauri-apps/create-tauri-app/issues"
  },
  "homepage": "https://github.com/tauri-apps/create-tauri-app#readme",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "create-tauri-app",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "i686-pc-windows-msvc"
      ]
    }
  },
  "license": "Apache-2.0 OR MIT",
  "devDependencies": {
    "@napi-rs/cli": "2.13.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "bin": {
    "create-tauri-app": "create-tauri-app.js"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm --gh-release-id $RELEASE_ID",
    "version": "napi version"
  },
  "optionalDependencies": {
    "create-tauri-app-win32-x64-msvc": "2.7.3",
    "create-tauri-app-darwin-x64": "2.7.3",
    "create-tauri-app-linux-x64-gnu": "2.7.3",
    "create-tauri-app-darwin-arm64": "2.7.3",
    "create-tauri-app-linux-arm64-gnu": "2.7.3",
    "create-tauri-app-linux-arm64-musl": "2.7.3",
    "create-tauri-app-linux-arm-gnueabihf": "2.7.3",
    "create-tauri-app-linux-x64-musl": "2.7.3",
    "create-tauri-app-win32-ia32-msvc": "2.7.3"
  }
}